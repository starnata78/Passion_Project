
@{
    ViewBag.Title = "Create";
}

<div class="go-link"><a href="/Owner/List">Go back to list</a></div>

<h2 class="table-title">Create Owner</h2>

<form action="/Owner/Create" method="POST">
    <div>
        <label>First Name:</label> <input type="text" name="First_Name" />
    </div>
    <div>
        <label>Last Name:</label> <input type="text" name="Last_Name" />
    </div>
    <div>
        <label>Email:</label> <input type="text" name="Email" />
    </div>
    <div>
        <button class="btn" style="background:#ff0000; color:#ffffff;" name="Create Owner">Create Owner</button>
    </div>

</form>

